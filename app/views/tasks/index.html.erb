<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<% @tasks.sort_by{ |t| [t.project.title, t.title, t.status] }.each do |task| %>
    <h2 ><%= task.title %></h2>
    <p><%= task.status %></p>
    <p><%= task.user.email %></p>
    <p><%= task.project.title %></p>
    <%= link_to 'Edit', edit_task_path(task) %>
    <%= link_to 'Delete', task, method: :delete %>
<% end %>

<%= render 'tasks/create' %>r